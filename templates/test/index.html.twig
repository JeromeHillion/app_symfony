{% extends 'base.html.twig' %}

{% block title %}Hello TestController!{% endblock %}

{% block body %}
<div class="dataPopularMovies">

{% for dataPopularMovie in dataPopularMovies %}

<div class="dataPopularMovie">
<div class="backdrop_path">
 <img src="https://image.tmdb.org/t/p/w500/{{dataPopularMovie.backdrop_path}}" alt="backdrop_path"/>

</div> 

<div class="title">
<h1>{{dataPopularMovie.title}}</h1>
</div>

<div class="release_date">
<time>{{dataPopularMovie.release_date}}</time>
</div>

<div class="overwiew">
<p>{{dataPopularMovie.overview}}</p>
</div>
</div>



{% endfor %}

</div>
{% endblock %}
